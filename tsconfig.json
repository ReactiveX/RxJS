{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "rxjs": [
        "./src"
      ],
      "rxjs/*": [
        "./src/*",
        "./legacy-reexport/*"
      ],
      "rxjs-compat": [
        "./compat/index"
      ],
      "rxjs-compat/*": [
        "./compat/*"
      ]
    }
  },
  "references": [
    {
      "path": "./tsconfig/tsconfig.cjs.json"
    }
  ],
  "exclude": [
    "dist",
    "dist-compat",
    "docs_app",
    "spec-dtslint"
  ]
}